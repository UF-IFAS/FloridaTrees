<rules moduleSOID="TREES Palm check rules rule">
<rule SOID="plams south 2">
<ruledefinition definition="if (palmcheck) is on the facts list and if zone is less than 8a and greater than 11 then goto TREES pool no palms"/>
<condition>
<mathtemplate type="and">
  <argument1>
<fact SOID="palmcheck"/>
  </argument1>
  <argument2>
  <mathtemplate type="parenthesis">
    <argument>
      <mathtemplate type="and">
      <argument1>
          <mathtemplate type="lessthan">
        <argument1>
      <fact SOID="zn"/>
        </argument1>
        <argument2>
      <factvalue type="integer" value="7"/>
        </argument2>
      </mathtemplate>

      </argument1>
      <argument2>
          <mathtemplate type="greaterthan">
        <argument1>
      <fact SOID="zn"/>
        </argument1>
        <argument2>
      <factvalue type="integer" value="13"/>
        </argument2>
      </mathtemplate>

      </argument2>
    </mathtemplate>

    </argument>
  </mathtemplate>

  </argument2>
</mathtemplate>

</condition>
<actions>
<question soid="TREES pool no palms"/>
<halt/>
</actions>
</rule>

<rule SOID="palms south">
<ruledefinition definition="if palmcheck is on the facts list and if zone is greater than or equal to 8a and less than or equal to 11 then goto TREES pool palms."/>
<condition>
<mathtemplate type="and">
  <argument1>
<fact SOID="palmcheck"/>
  </argument1>
  <argument2>
  <mathtemplate type="parenthesis">
    <argument>
      <mathtemplate type="and">
      <argument1>
          <mathtemplate type="greaterandequal">
        <argument1>
      <fact SOID="zn"/>
        </argument1>
        <argument2>
      <factvalue type="integer" value="7"/>
        </argument2>
      </mathtemplate>

      </argument1>
      <argument2>
          <mathtemplate type="lessandequal">
        <argument1>
      <fact SOID="zn"/>
        </argument1>
        <argument2>
      <factvalue type="integer" value="13"/>
        </argument2>
      </mathtemplate>

      </argument2>
    </mathtemplate>

    </argument>
  </mathtemplate>

  </argument2>
</mathtemplate>

</condition>
<actions>
<question soid="TREES pool palms"/>
<halt/>
</actions>
</rule>

</rules>
