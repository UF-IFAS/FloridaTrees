<rules moduleSOID="TREES Salt rules rule">
<rule SOID="salt-question-2">
<ruledefinition definition="if (salt Q) is on the facts list and if zone is greater to 8a then goto TREES vandalism question"/>
<condition>
<mathtemplate type="and">
  <argument1>
  <mathtemplate type="parenthesis">
    <argument>
      <mathtemplate type="equal">
      <argument1>
    <fact SOID="salt"/>
      </argument1>
      <argument2>
    <factvalue type="string" value="{1=Q}"/>
      </argument2>
    </mathtemplate>

    </argument>
  </mathtemplate>

  </argument1>
  <argument2>
  <mathtemplate type="parenthesis">
    <argument>
      <mathtemplate type="greaterthan">
      <argument1>
    <fact SOID="zn"/>
      </argument1>
      <argument2>
    <factvalue type="integer" value="7"/>
      </argument2>
    </mathtemplate>

    </argument>
  </mathtemplate>

  </argument2>
</mathtemplate>

</condition>
<actions>
<question soid="TREES vandalism question"/>
</actions>
</rule>

<rule SOID="salt-question-1">
<ruledefinition definition="if (salt Q) is on the facts list and if zone is less than or equal to 8a then goto TREES saltground question"/>
<condition>
<mathtemplate type="and">
  <argument1>
  <mathtemplate type="parenthesis">
    <argument>
      <mathtemplate type="equal">
      <argument1>
    <fact SOID="salt"/>
      </argument1>
      <argument2>
    <factvalue type="string" value="{1=Q}"/>
      </argument2>
    </mathtemplate>

    </argument>
  </mathtemplate>

  </argument1>
  <argument2>
  <mathtemplate type="parenthesis">
    <argument>
      <mathtemplate type="lessandequal">
      <argument1>
    <fact SOID="zn"/>
      </argument1>
      <argument2>
    <factvalue type="integer" value="7"/>
      </argument2>
    </mathtemplate>

    </argument>
  </mathtemplate>

  </argument2>
</mathtemplate>

</condition>
<actions>
<question soid="TREES saltground"/>
</actions>
</rule>

</rules>
